{
    "name": "stein-boilerplate",
    "version": "1.0.0",
    "description": "stein.js boilerplate app",
    "main": "assets/",
    "author": "Milan van As <milanvanas@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "NODE_ENV=development webpack --progress",
        "build:production": "NODE_ENV=production webpack -p --progress",
        "watch": "NODE_ENV=development webpack --progress --watch",
        "watch:production": "NODE_ENV=production webpack --progress --watch -p",
        "test": "NODE_ENV=test mocha --require ignore-styles --require babel-register --colors ./app/**/*.test.js",
        "test:watch": "NODE_ENV=test mocha --require ignore-styles --require babel-register --colors -w ./app/**/*.test.js"
    },
    "dependencies": {
        "jquery": "^3.2.1",
        "stein": "^1.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "chai": "^4.1.2",
        "css-loader": "^0.28.8",
        "expose-loader": "^0.7.4",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "ignore-styles": "^5.0.1",
        "mocha": "^5.0.0",
        "node-sass": "^4.7.2",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "webpack": "^3.10.0",
        "webpack-dev-server": "^2.11.0",
        "webpack-jarvis": "^0.2.2",
        "webpack-watched-glob-entries-plugin": "^1.1.0"
    }
}
